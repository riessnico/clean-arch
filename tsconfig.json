{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@/*":["*"]
    },
    "allowJs": true,
    "strict": false,
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true,
  },
  "include": ["./src/"],
}
